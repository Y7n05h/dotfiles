{
  "workbench.colorTheme": "One Dark Pro",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": true,
  "workbench.iconTheme": "material-icon-theme",
  "explorer.confirmDelete": false,
  "git.confirmSync": false,
  "workbench.enableExperiments": false,
  "update.mode": "none",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.tree.renderIndentGuides": "always",
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/.git": false
  },
  "files.autoSave": "onFocusChange",
  "editor.tabCompletion": "on",
  "debug.onTaskErrors": "abort",
  "editor.mouseWheelZoom": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.fontSize": 15,
  "workbench.sideBar.location": "left",
  "terminal.integrated.copyOnSelection": true,
  "editor.fontFamily": "'Source Code Pro','等距更纱黑体 SC','SauceCodePro Nerd Font Mono'",
  "editor.codeLensFontFamily": "'Source Code Pro','等距更纱黑体 SC','SauceCodePro Nerd Font Mono'",
  "editor.inlineHints.fontFamily": "'Source Code Pro','等距更纱黑体 SC','SauceCodePro Nerd Font Mono'",
  "debug.console.fontFamily": "'Source Code Pro','等距更纱黑体 SC','SauceCodePro Nerd Font Mono'",
  "update.showReleaseNotes": false,
  "explorer.incrementalNaming": "smart",
  "git.autofetch": true,
  "files.trimTrailingWhitespace": true,
  "extensions.ignoreRecommendations": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "workbench.editor.untitled.hint": "hidden",
  "CppHelper.HeaderGuardPattern": "__{FILE}_HPP",
  "terminal.integrated.rightClickBehavior": "copyPaste",
  "files.autoGuessEncoding": true,
  "clangd.onConfigChanged": "restart",
  "[c]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
  },
  "code-runner.runInTerminal": true, // 设置成false会在“输出”中输出，无法输入
  "code-runner.preserveFocus": false,
  "files.associations": {
    "*.h": "c",
    "*.hpp": "cpp",
    "*.json": "jsonc"
  },
  "files.autoSaveDelay": 10000,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.accessibilitySupport": "off",
  "git.enableSmartCommit": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorCustomizations": {
    "activityBar.foreground": "#FFFFFF",
    "activityBarBadge.background": "#2979FF",
    "activityBar.activeBorder": "#2979FF",
    "list.activeSelectionForeground": "#2979FF",
    "list.inactiveSelectionForeground": "#2979FF",
    "list.highlightForeground": "#2979FF",
    "scrollbarSlider.activeBackground": "#2979FF50",
    "editorSuggestWidget.highlightForeground": "#2979FF",
    "textLink.foreground": "#2979FF",
    "progressBar.background": "#2979FF",
    "pickerGroup.foreground": "#2979FF",
    "tab.activeBorder": "#2979FF",
    "notificationLink.foreground": "#2979FF",
    "editorWidget.resizeBorder": "#2979FF",
    "editorWidget.border": "#2979FF",
    "settings.modifiedItemIndicator": "#2979FF",
    "settings.headerForeground": "#2979FF",
    "panelTitle.activeBorder": "#2979FF",
    "breadcrumb.activeSelectionForeground": "#2979FF",
    "menu.selectionForeground": "#2979FF",
    "menubar.selectionForeground": "#2979FF",
    "editor.findMatchBorder": "#2979FF",
    "selection.background": "#2979FF40",
    "statusBarItem.remoteBackground": "#2979FF"
  },
  "tabnine.experimentalAutoImports": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "python.analysis.cacheFolderPath": "/tmp/python",
  "python.formatting.autopep8Path": "/usr/bin/autopep8",
  "settingsSync.ignoredExtensions": [
    "ms-vscode-remote.remote-wsl"
  ],
  "shellformat.path": "/usr/bin/shfmt",
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "python.analysis.completeFunctionParens": true,
  "python.autoComplete.addBrackets": true,
  "python.analysis.memory.keepLibraryAst": true,
  "python.languageServer": "Jedi",
  "python.analysis.typeCheckingMode": "off",
  "window.menuBarVisibility": "toggle",
  "asm-code-lens.donated": true,
  "go.toolsManagement.checkForUpdates": "off",
  "commentTranslate.multiLineMerge": true,
  "commentTranslate.source": "Google",
  "commentTranslate.targetLanguage": "zh-CN",
  "go.enableCodeLens": {
    "references": true
  },
  "go.formatTool": "gofmt",
  "vscode-office.autoTheme": true,
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "clangd.detectExtensionConflicts": false,
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.linux": "/home/admin/dotfiles/vim/.config/nvim/vscinit.vim",
  "vscode-neovim.revealCursorScrollLine": true,
  "vscode-neovim.useCtrlKeysForInsertMode": false,
  "editor.lineNumbers": "relative"
}